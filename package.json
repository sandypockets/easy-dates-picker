{
  "name": "easy-dates-picker",
  "version": "0.1.2",
  "description": "A super lightweight, zero dependency date picker, written in vanilla JS",
  "author": "sandypockets",
  "license": "MIT",
  "main": "dist/datepicker.bundle.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "webpack",
    "test": "jest",
    "prettier": "prettier src/ --write && prettier example --write",
    "prep": "npm run prettier && npm run test && npm run build && git status"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sandypockets/easy-dates-picker.git"
  },
  "bugs": {
    "url": "https://github.com/sandypockets/easy-dates-picker/issues"
  },
  "homepage": "https://github.com/sandypockets/easy-dates-picker#readme",
  "keywords": [
    "date-picker",
    "calendar",
    "no-dependencies",
    "ui-component",
    "date-selection",
    "accessible",
    "datetime-picker"
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "testEnvironment": "jest-environment-jsdom"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.7.6",
    "prettier": "3.1.0",
    "style-loader": "^3.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
